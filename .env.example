# Database Configuration
DATABASE_URL=postgresql://postgres:postgres123@localhost:5432/social_commerce
POSTGRES_DB=social_commerce
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200

# Service Secret Keys
USER_SERVICE_SECRET_KEY=your-user-service-secret-key-here
PRODUCT_SERVICE_SECRET_KEY=your-product-service-secret-key-here
ORDER_SERVICE_SECRET_KEY=your-order-service-secret-key-here
SOCIAL_SERVICE_SECRET_KEY=your-social-service-secret-key-here
PAYMENT_SERVICE_SECRET_KEY=your-payment-service-secret-key-here
